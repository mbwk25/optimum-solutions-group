    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.3",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8",
    "glob": "^11.0.3"
  },
  "overrides": {
    "esbuild": "^0.25.9"
