{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "optimum-solutions-group",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "set NODE_ENV=production && vite build --config vite.config.prod.ts",
    "build:optimized": "node scripts/build-optimized.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && start coverage/lcov-report/index.html",
    "test:ui": "jest --testPathPattern=shared/ui",
    "test:components": "jest --testPathPattern=shared/components",
    "test:features": "jest --testPathPattern=features",
    "test:hooks": "jest --testPathPattern=hooks",
    "test:utils": "jest --testPathPattern=utils",
    "test:accessibility": "jest --testNamePattern='accessibility|a11y'",
    "test:performance": "jest --testNamePattern='performance|benchmark'",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:debug": "jest --no-cache --verbose",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze:bundle": "vite build --mode production && npm run bundle:analyze",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "test:e2e": "start-server-and-test dev http://localhost:8080 cypress:run",
    "test:e2e:open": "start-server-and-test dev http://localhost:8080 cypress:open",
    "test:e2e:components": "cypress run --spec 'cypress/e2e/components/**/*'",
    "test:e2e:integration": "cypress run --spec 'cypress/e2e/integration/**/*'",
    "test:e2e:accessibility": "cypress run --spec 'cypress/e2e/accessibility/**/*'",
    "test:cypress:accessibility": "start-server-and-test dev http://localhost:8080 'cypress run --spec cypress/e2e/accessibility/**/*'",
    "test:accessibility:headed": "start-server-and-test dev http://localhost:8080 'cypress run --spec cypress/e2e/accessibility/**/* --headed'",
    "test:e2e:visual": "cypress run --spec 'cypress/e2e/visual/**/*'",
    "test:e2e:journeys": "cypress run --spec 'cypress/e2e/user-journeys/**/*'",
    "test:all": "npm run test && npm run test:e2e",
    "test:workflow": "node scripts/test-workflow.js",
    "test:workflow:local": "node scripts/test-local-workflow.js",
    "deploy:workflow": "node deploy-workflow.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
